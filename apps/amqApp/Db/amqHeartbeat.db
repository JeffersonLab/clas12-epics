record(ai,"$(P)_HEARTBEAT"){
  field(DTYP,"AMQR")
  field(INP,"$(P)_HEARTBEAT")
  field(PREC,"0")
  field(FLNK,"$(P)-DF")
}

record(dfanout, "$(P)-DF"){
  field(DOL,"0")
  field(OUTA,$(P)_ALIVE)
  }

record(calc,"$(P)_ALIVE") {
  field(CALC,"VAL+1")
  field(SCAN, "1 second")
  field(HIHI, "10")
  field(HIGH, "5")
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
}

