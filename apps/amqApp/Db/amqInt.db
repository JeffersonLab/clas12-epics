record(ai,"$(P)"){
  field(DTYP,"AMQR")
  field(INP,"$(K)")
  field(PREC,"0")
  field(DESC,"json $(K) - value")
  field(FLNK,"$(P)-DF")
}

record(dfanout, "$(P)-DF"){
  field(DOL,"0")
  field(OUTA,$(P)-Time)
  }

record(calc,"$(P)-Time") {
  field(DESC,"json $(K) - time since last msg")
  field(CALC,"VAL+1")
  field(SCAN, "1 second")
  field(HIHI, "20")
  field(HIGH, "10")
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
}

