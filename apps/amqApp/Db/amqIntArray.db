record(waveform,"$(P)"){
  field(DTYP,"AMQR")
  field(INP,"$(K)")
  field(NELM,"$(N)")
  field(FTVL,"LONG")
  field(DESC,"ActiveMQ message text")
  field(FLNK,"$(P)-DF")
}

record(dfanout, "$(P)-DF"){
  field(DOL,"0")
  field(OUTA,$(P)-Time)
  }

record(calc,"$(P)-Time") {
  field(DESC,"json $(K) - time since last msg")
  field(CALC,"VAL+1")
  field(SCAN, "1 second")
  field(HIHI, "$(THH)")
  field(HIGH, "$(TH)")
  field(HHSV, "$(HHSV)")
  field(HSV, "$(HSV)")
}
