record(acalcout,"$(P):acalcout") {
    field(INAA,"$(IN1) CPP MS")
    field(INBB,"$(IN2) CPP MS")
    field(CALC,"AA+BB")
    field(OUT,"$(P) CPP")
    field(OOPT,"Every Time")
    field(DOPT,"Use CALC")
    field(NELM,"$(N)")
}
record(waveform,"$(P)") {
    field(NELM,"$(N)")
    field(FTVL,"DOUBLE")
}

