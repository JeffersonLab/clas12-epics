
file "db/asy-generic.db" {
pattern { P                  INPLUS             INMINUS            }
        { B_MOLLER:slm:bca   moller_SLM_sum_pos moller_SLM_sum_neg }
}

file "db/asy-withsums-generic.db" {
pattern { P                  INPLUS     INMINUS     RESET        DISABLE }
        { B_MOLLER:clock:bca sum_plus_0 sum_minus_0 moller_reset moller_accumulate } 
}

