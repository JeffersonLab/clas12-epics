
record(calc,"B_BONUS_INTLK:bypassed") {
    field(SCAN,"2 second")
    field(INPA,"B_BONUS_INTLK_Flow:bypass")
    field(INPB,"B_BONUS_INTLK_FlowSetDiff:bypass")
    field(INPC,"B_BONUS_INTLK_RTPC_Press:bypass")
    field(INPD,"B_BONUS_INTLK_Diff_Press:bypass")
    field(INPE,"B_BONUS_INTLK_Buffer_Press:bypass")
    field(INPF,"B_BONUS_HV_GEM:dv:intlk:bypass")
    field(CALC,"A+B+C+D+E+F")
    field(HIGH,1)
    field(HSV,"MAJOR")
    info(autosaveFields_pass0,"HIGH")
}
record(calc,"B_BONUS_INTLK:tripped") {
    field(SCAN,"2 second")
    field(INPA,"B_BONUS_INTLK_Flow:status")
    field(INPB,"B_BONUS_INTLK_FlowSetDiff:status")
    field(INPC,"B_BONUS_INTLK_RTPC_Press:status")
    field(INPD,"B_BONUS_INTLK_Diff_Press:status")
    field(INPE,"B_BONUS_INTLK_Buffer_Press:status")
    field(INPF,"B_BONUS_HV_GEM1CAT:watchdog:calc")
    field(INPG,"B_BONUS_HV_GEM1:watchdog:calc")
    field(INPH,"B_BONUS_HV_GEM2:watchdog:calc")
    field(INPI,"B_BONUS_HV_GEM3:watchdog:calc")
    field(INPJ,"B_BONUS_HV_GEM12:watchdog:calc")
    field(INPK,"B_BONUS_HV_GEM23:watchdog:calc")
    field(CALC,"A+B+C+D+E+((F+G+H+I+J+K)>0?1:0)")
    field(HIGH,1)
    field(HSV,"MAJOR")
    info(autosaveFields_pass0,"HIGH")
}

