record(ai, "$(P)$(R)N2Flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ai, "$(P)$(R)Press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ai, "$(P)$(R)Humidity") {
    field(EGU,  "%")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

