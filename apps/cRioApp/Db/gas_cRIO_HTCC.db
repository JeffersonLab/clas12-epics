record(ai, "$(P)$(R)CO2_FLOW") {
    field(EGU,  "lpm")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ai, "$(P)$(R)CO2_FLOW_SET") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)CO2_FLOW_WRITESET") {
    field(EGU, "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)H2O") {
    field(EGU,  "ppm")
    field(PREC, "0")
    field(HIHI, "50")
    field(HHSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ai, "$(P)$(R)PRESS") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(HIHI, "0.1")
    field(HHSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(mbbi, "$(P)$(R)TYPE") {
    field(ZRST,"CO2")
    field(ONST,"Nitrogen")
    field(TWST,"Dry Air")
    info(autosaveFields_pass0,"VAL")
}

