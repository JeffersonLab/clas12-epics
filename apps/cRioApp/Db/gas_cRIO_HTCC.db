record(ai, "$(P)$(R)CO2Flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ao, "$(P)$(R)CO2FlowSet") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)H2O") {
    field(EGU,  "ppm")
    field(PREC, "0")
    field(HIHI, "50")
    field(HHSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ai, "$(P)$(R)Press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(HIHI, "0.1")
    field(HHSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

