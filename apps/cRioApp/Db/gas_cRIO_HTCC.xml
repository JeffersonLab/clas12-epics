<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config name="HallB">
    <component name="HTCC">
        <component name="Gas">
            <automated_action>
                <title>Auto Email Youri Sharibian</title>
                <details>mailto:youris@jlab.org</details>
                <delay>600</delay>
            </automated_action>
            <automated_action>
                <title>Auto Email Will Phelps</title>
                <details>mailto:wphelps@jlab.org</details>
                <delay>600</delay>
            </automated_action>
            <automated_action>
                <title>Auto Email Nick Markov</title>
                <details>mailto:markov@jlab.org</details>
                <delay>600</delay>
            </automated_action>
            <pv name="B_DET_HTCC_GAS_PRESS">
                <description>HTCC Pressure</description>
                <latching>true</latching>
                <annunciating>true</annunciating>
                <delay>60</delay>
                <count>60</count>
            </pv>
            <pv name="B_DET_HTCC_GAS_CO2_FLOW">
                <description>HTCC CO2 Flow</description>
                <latching>true</latching>
                <annunciating>true</annunciating>
                <delay>60</delay>
                <count>60</count>
            </pv>
            <pv name="B_DET_HTCC_GAS_H2O">
                <description>HTCC H2O Concentration</description>
                <latching>true</latching>
                <annunciating>true</annunciating>
                <delay>60</delay>
                <count>60</count>
            </pv>
        </component>
    </component>
</config>
