record(ai, "$(P)$(R)Dist_Flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)Dist_FlowSet") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)Dist_N2Flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)Dist_N2FlowSet") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)Dist_Temp") {
    field(EGU,  "F")
    field(PREC, "2")
    field(LOLO, "-40")
    field(HIHI, "-10")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
}

record(ai, "$(P)$(R)CtrlTank_Press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(LOLO, "0.025")
    field(HIHI, "2.0")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
}

record(ai, "$(P)$(R)RetTank_Press") {
    field(EGU,  "psi")
    field(PREC, "2")
    field(HIHI, "20")
    field(HHSV, "MAJOR")
}

