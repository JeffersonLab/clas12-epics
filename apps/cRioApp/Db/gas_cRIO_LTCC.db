record(ai, "$(P)$(R)dist_flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)dist_flow_set") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)dist_N2_flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)dist_N2_flow_set") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)dist_temp") {
    field(EGU,  "F")
    field(PREC, "2")
    field(LOLO, "-40")
    field(HIHI, "-10")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
}

record(ai, "$(P)$(R)ctrl_tank_press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(LOLO, "0.025")
    field(HIHI, "2.0")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
}

record(ai, "$(P)$(R)rtn_tank_press") {
    field(EGU,  "psi")
    field(PREC, "2")
    field(HIHI, "20")
    field(HHSV, "MAJOR")
}

record(ai, "$(P)$(R)rtn_tank_press") {
    field(EGU,  "psi")
    field(PREC, "2")
    field(HIHI, "20")
    field(HHSV, "MAJOR")
}

