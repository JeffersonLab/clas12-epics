record(ai, "$(P)$(R)$(SEC)flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ao, "$(P)$(R)$(SEC)flow_set") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)$(SEC)press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(LOLO, "0.025")
    field(HIHI, "2.0")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
}

