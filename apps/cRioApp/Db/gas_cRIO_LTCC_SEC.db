record(ai, "$(P)$(R)$(SEC)Flow") {
    field(EGU,  "lpm")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ao, "$(P)$(R)$(SEC)FlowSet") {
    field(EGU,  "lpm")
    field(PREC, "2")
}

record(ai, "$(P)$(R)$(SEC)Press") {
    field(EGU,  "iwc")
    field(PREC, "2")
    field(LOLO, "0.025")
    field(HIHI, "2.0")
    field(LLSV, "MAJOR")
    field(HHSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

