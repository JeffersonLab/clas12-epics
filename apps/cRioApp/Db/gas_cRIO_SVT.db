record(ai, "$(P)$(R)N2_SUP_FLOW") {
    field(EGU,  "lpm")
    field(PREC, "2")
    field(LOLO, "1")
    field(LLSV, "MAJOR")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

record(ao, "$(P)$(R)N2_SUP_FLOW_SET") {
    field(EGU,  "lpm")
    field(PREC, "2")
    field(DRVH,"20")
    info(autosaveFields_pass0,"VAL DRVH DRVL")
}

