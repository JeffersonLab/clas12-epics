record(stringin,"$(P):msg") {
	field(VAL,"Uninitialized")
}
record(bi,"$(P):clear") {
	field(VAL,0)
}
record(bi,"$(P):enable") {
	field(VAL,0)
	field(ZNAM,"Disabled")
	field(ONAM,"Enabled")
}
record(ai,"$(P):errors") {
	field(VAL,0)
}
record(ao,"$(P):dwell") {
	field(VAL,1)
    field(EGU,"s")
    field(DRVL,0.0001)
}
record(ai,"$(P):timer") {
	field(VAL,0)
    field(EGU,"s")
}
record(ao,"$(P):ticsperbin") {
    field(VAL,10)
    field(PINI,1)
    field(PREC,0)
    field(DRVL,1)
    field(DRVH,10000)
    field(FLNK,"$(P):binsize.PROC")
}
record(ai,"$(P):resolution") {
    field(VAL,"195.3125")
    field(EGU,"ps")
}
record(ai,"$(P):prescale") {
    field(VAL,"1")
}
record(calc,"$(P):binsize") {
    field(INPA,"$(P):ticsperbin")
    field(INPB,"$(P):resolution")
    field(PREC,4)
    field(EGU,"us")
    field(CALC,"A<1?B/1000000:A*B/1000000")
    field(FLNK,"$(P):binsize:ps.PROC")
}
record(calc,"$(P):binsize:ps") {
    field(INPA,"$(P):binsize")
    field(PREC,4)
    field(EGU,"ps")
    field(CALC,"A*1000000")
    field(CALC,"A*1000000")
}
record(ao,"$(P):tmin") {
    field(EGU,"us")
    field(VAL,0)
}
record(ao,"$(P):tmax") {
    field(EGU,"us")
    field(VAL,100)
}
record(mbbi,"$(P):status") {
	field(ZRST,"Uninitialized")
	field(ONST,"Initializing")
	field(TWST,"Disabled")
	field(THST,"Reading")
	field(FRST,"Clearing")
	field(FVST,"Error")
    field(SXST,"Waiting for PVs")
    field(ZRSV,"MAJOR")
    field(ONSV,"MINOR")
    field(TWSV,"MINOR")
    field(THSV,"NO_ALARM")
    field(FRSV,"NO_ALARM")
    field(FVSV,"MAJOR")
    field(SXSV,"MAJOR")
}
record(waveform,"$(P):xaxis") {
    field(NELM,1000000)
    field(FTVL,"FLOAT")
}
record(ai,"$(P):hz:0") {
	field(EGU,"Hz")
}
