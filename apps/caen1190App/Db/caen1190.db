record(ao,"$(P):dwell") {
	field(VAL,1)
    field(EGU,"s")
    field(DRVL,0.1)
    field(DRVH,600)
}
record(ai,"$(P):timer") {
	field(VAL,0)
    field(EGU,"s")
}
record(ao,"$(P):prescale") {
    field(DRVL,1)
    field(DRVH,1000000)
    field(VAL,"1")
}
record(ao,"$(P):ticsperbin") {
    field(VAL,8)
    field(PINI,1)
    field(PREC,0)
    field(DRVL,1)
    field(DRVH,10000)
    field(FLNK,"$(P):binsize.PROC")
}
record(ai,"$(P):resolution") {
    field(VAL,"195.3125")
    field(EGU,"ps")
    field(FLNK,"$(P):binsize.PROC")
}
record(calc,"$(P):binsize") {
    field(INPA,"$(P):ticsperbin")
    field(INPB,"$(P):resolution")
    field(PREC,4)
    field(EGU,"us")
    field(CALC,"A<1?B/1000000:A*B/1000000")
    field(FLNK,"$(P):binsize:ps.PROC")
}
record(calc,"$(P):binsize:ps") {
    field(INPA,"$(P):binsize")
    field(PREC,4)
    field(EGU,"ps")
    field(CALC,"A*1000000")
}
record(ao,"$(P):tmin") {
    field(EGU,"us")
    field(PREC,3)
    field(VAL,0)
}
record(ao,"$(P):tmax") {
    field(EGU,"us")
    field(PREC,3)
    field(VAL,100)
}
record(mbbi,"$(P):status") {
	field(ZRST,"Uninitialized")
	field(ONST,"Initializing")
	field(TWST,"Disabled")
	field(THST,"Reading")
	field(FRST,"Clearing")
	field(FVST,"Error")
    field(SXST,"Waiting for PVs")
    field(SVST,"Transforming")
    field(ZRSV,"MAJOR")
    field(ONSV,"MINOR")
    field(TWSV,"MINOR")
    field(THSV,"NO_ALARM")
    field(FRSV,"NO_ALARM")
    field(FVSV,"MAJOR")
    field(SXSV,"MAJOR")
    field(SVSV,"NO_ALARM")
}
