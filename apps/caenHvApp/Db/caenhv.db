record(bigsub, "B_$(CrName)_Sl$(Sl)_Ch$(Ch)") 
{
   field(PRIO,"LOW")
   field(DESC,"$(Det)_$(Element)")
   field(INAM,"InitChannel")
   field(INPA,"$(Cr)")
   field(INPB,"$(Sl)")
   field(HIGH,"50")
   field(INPC,"$(Ch)")
   field(HSV,"MAJOR")
   field(PREC,"1")
   field(SNAM,"ScanChannel")
   field(SCAN,"2 second")
   field(ASG,"EXPERT")
   alias("B_$(Sys)_$(Det)_$(Element)")
}

record(ao, "B_$(CrName)_Sl$(Sl)_Ch$(Ch):Vmon")
{
   field(DOL, "B_$(CrName)_Sl$(Sl)_Ch$(Ch).F CPP NMS")
   field(OMSL,"closed_loop")
   field(PREC,"2")
   alias("B_$(Sys)_$(Det)_$(Element):Vmon")
}   

