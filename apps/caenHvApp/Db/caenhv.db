record(bigsub, "B_$(CrName)_Sl$(Sl)_Ch$(Ch)") 
{
   field(PRIO,"LOW")
   field(DESC,"$(Det)_$(Element)")
   field(INAM,"InitChannel")
   field(INPA,"$(Cr)")
   field(INPB,"$(Sl)")
   field(HIGH,"50")
   field(INPC,"$(Ch)")
   field(HSV,"MAJOR")
   field(PREC,"1")
   field(SNAM,"ScanChannel")
   field(SCAN,"2 second")
   alias("B_$(Sys)_$(Det)_$(Element)")
}

record(bo, "B_$(CrName)_Sl$(Sl)_Ch$(Ch):pwonoff") 
{
   field(DTYP,"CAEN_HV")
   field(DESC,"#PB")
   field(OUT,"$(CScode) $(pwonoff)")
   field(OMSL,"supervisory")
   field(ZNAM,"OFF")
   field(ONAM,"ON")
   field(SCAN,"Passive")
   alias("B_$(Sys)_$(Det)_$(Element):pwonoff")
}

