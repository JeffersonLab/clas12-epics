record(bigsub, "B_HW_$(CrName)_Sl$(Sl)_Ch$(Ch)") 
{
   field(PRIO,"LOW")
   field(DESC,"$(Det)_LV_$(Element)")
   field(INAM,"InitChannel")
   field(INPA,"$(Cr)")
   field(INPB,"$(Sl)")
   field(INPC,"$(Ch)")
field(LSV,"MINOR")
field(HSV,"MINOR")
field(LLSV,"MAJOR")
field(HHSV,"MAJOR")
field(HIGH,"10")
field(LOW,"-10")
field(HIHI, "10000")
field(LOLO,"-10000")
   field(PREC,"1")
   field(SNAM,"ScanChannel")
   field(SCAN,"2 second")
   alias("B_DET_$(Det)_$(Sys)_$(Element)")
   alias("B_SYS_$(Sys)_$(Det)_$(Element)")
info(autosaveFields_pass0,"HSV LSV HIGH LOW DESC")
}


