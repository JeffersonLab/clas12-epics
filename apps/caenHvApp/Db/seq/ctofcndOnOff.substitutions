file "db/ctofOnOff-1side.db" {
pattern { SIDE ONOFF VAL }
    { U OFF 0 }
    { D OFF 0 }
    { U ON 1 }
    { D ON 1 }
}
file "db/cndOnOff-1side.db" {
pattern { IMO ONOFF VAL }
    { Inner OFF 0 }
    { Middle OFF 0 }
    { Outer OFF 0 }
    { Inner ON 1 }
    { Middle ON 1 }
    { Outer ON 1 }
}

