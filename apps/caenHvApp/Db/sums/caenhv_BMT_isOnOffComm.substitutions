file "db/caenhv_BMT_isOnOffComm-1type.db" {
pattern { DET SEC   TYPE }
        { BMT 1 DRIFT }
        { BMT 2 DRIFT }
        { BMT 3 DRIFT }
        { BMT 1 STRIP }
        { BMT 2 STRIP }
        { BMT 3 STRIP }
}

file "db/caenhv_BMT_isOnOffComm-1sec.db" {
pattern { SEC }
        { 1 }
        { 2 }
        { 3 }
        { 4 }
        { 5 }
        { 6 }
}
