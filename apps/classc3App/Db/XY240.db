# database for e2 targets, use XYCOM240 ttl i/o board for 
# control and readback

record(bo,"tagger_dump_lamp") {
	field(DTYP,"XYCOM-240M")
	field(OUT, "#C1 S0 @0")
	field(ZNAM,"OFF")
	field(ONAM,"ON")
	field(VAL,"1")
	field(ASG,"MCC_HARPS")
}
record(bo,"tagger_cross_lamp") {
	field(DTYP,"XYCOM-240M")
	field(OUT, "#C1 S0 @1")
	field(ZNAM,"OFF")
	field(ONAM,"ON")
	field(VAL,"1")
	field(ASG,"MCC_HARPS")
}
record(bo,"otr1") {
	field(DTYP,"XYCOM-240M")
	field(OUT, "#C1 S0 @2")
	field(ZNAM,"OUT")
	field(ONAM,"IN")
	field(VAL,"1")
}
record(bo,"tagger_cross_viewer") {
	field(DTYP,"XYCOM-240M")
	field(OUT, "#C1 S0 @3")
	field(ZNAM,"OFF")
	field(ONAM,"ON")
	field(VAL,"1")
	field(ASG,"MCC_HARPS")
}
record(bo,"otr2") {
	field(DTYP,"XYCOM-240M")
	field(OUT, "#C1 S0 @4")
	field(ZNAM,"OUT")
	field(ONAM,"IN")
	field(VAL,"1")
}
record(bi,"fv-cvs") {
	field(DTYP,"XYCOM-240M")
	field(INP, "#C1 S2 @0")
	field(ZNAM,"CLOSED")
	field(ONAM,"OPEN")
	field(ZSV,"MAJOR")
	field(OSV,"NO_ALARM")
	field(COSV,"NO_ALARM")
	field(SCAN,"1 second")
}
record(bi,"h2o_lcw_1") {
	field(DTYP,"XYCOM-240M")
	field(INP, "#C1 S4 @0")
	field(ZNAM,"DRY")
	field(ONAM,"WET")
	field(ZSV,"NO_ALARM")
	field(OSV,"MAJOR")
	field(COSV,"NO_ALARM")
	field(SCAN,"1 second")
}
record(bi,"h2o_lcw_2") {
	field(DTYP,"XYCOM-240M")
	field(INP, "#C1 S4 @1")
	field(ZNAM,"DRY")
	field(ONAM,"WET")
	field(ZSV,"NO_ALARM")
	field(OSV,"MAJOR")
	field(COSV,"NO_ALARM")
	field(SCAN,"1 second")
}

