# database for e2 targets, use XYCOM240 ttl i/o board for 
# control and readback


record(bo,"tagger_dump_lamp")
{
	field(ASG,"MCC_HARPS")
	field(DESC,"binary output record")
	field(SCAN,"Passive")
	field(PINI,"NO")
	field(VAL,"1")
	field(PHAS,"0")
	field(EVNT,"0")
	field(DTYP,"XYCOM-240M")
	field(SDIS,"0.000000000000000e+00")
	field(DISS, "NO_ALARM")
	field(PRIO, "LOW")
	field(FLNK, "0.0")
	field(OMSL, "supervisory")
	field(DOL, "0.000000000000000e+00")
	field(OUT, "#C1 S0 @0")
	field(HIGH,"0.0")
	field(ZNAM,"OFF")
	field(ONAM,"ON")
	field(ZSV,"NO_ALARM")
	field(OSV,"NO_ALARM")
	field(COSV,"NO_ALARM")
	field(SIOL, "0.000000000000000e+00")
	field(SIML, "0.000000000000000e+00")
	field(SIMS, "NO_ALARM")
	field(IVOA, "Continue normally")
#	field(IVOV, "0.000000000000000e+00")
}
