record(calc,"B_DET_AHDC_INTLK:bypass") {
    field(SCAN,"5 second")
    field(INPA,"B_DET_AHDC_INTLK_Sl1:bypass")
    field(INPB,"B_DET_AHDC_INTLK_Sl2:bypass")
    field(CALC,"A+B")
    field(HIGH,"1")
    field(HSV,"MAJOR")
}
record(calc,"B_DET_AHDC_INTLK:status") {
    field(SCAN,"5 second")
    field(INPA,"B_DET_AHDC_INTLK_Sl1:status MS")
    field(INPB,"B_DET_AHDC_INTLK_Sl2:status MS")
    field(CALC,"A+B")
}
record(calc,"B_DET_AHDC_INTLK_Sl$(S):bypass") {
    field(SCAN,"5 second")
    field(INPA,"B_DET_AHDC_INTLK_Sl$(S):Current_A:bypass")
    field(INPB,"B_DET_AHDC_INTLK_Sl$(S):XTmpV6_deg:bypass")
    field(INPC,"B_DET_AHDC_INTLK_Sl$(S):MTmpInt_deg:bypass")
    field(INPD,"B_DET_AHDC_INTLK_Sl$(S):TmpSdX_deg:bypass")
    field(INPE,"B_DET_AHDC_INTLK_Sl$(S):TmpSdA_deg:bypass")
    field(CALC,"A+B+C+D+E")
    field(HIGH,"1")
    field(HSV,"MAJOR")
}
record(calc,"B_DET_AHDC_INTLK_Sl$(S):status") {
    field(SCAN,"5 second")
    field(INPA,"B_DET_AHDC_INTLK_Sl$(S):Current_A:status MS")
    field(INPB,"B_DET_AHDC_INTLK_Sl$(S):XTmpV6_deg:status MS")
    field(INPC,"B_DET_AHDC_INTLK_Sl$(S):MTmpInt_deg:status MS")
    field(INPD,"B_DET_AHDC_INTLK_Sl$(S):TmpSdX_deg:status MS")
    field(INPE,"B_DET_AHDC_INTLK_Sl$(S):TmpSdA_deg:status MS")
    field(CALC,"A+B+C+D+E")
}

