record(ao,"$(M)_command") {
    field(ASG,"MCC_HARPS")

}
record(ao,"$(M)_position_a") {
    field(ASG,"MCC_HARPS")
    field(VAL,"$(start_at)")
    field(PREC,"4")
}
record(ao,"$(M)_position_b") {
    field(ASG,"MCC_HARPS")
    field(VAL,"$(end_at)")
    field(PREC,"4")
}
record(ao,"$(M)_speed_to_a") {
    field(ASG,"MCC_HARPS")
   field(VAL,"$(start_speed)")
    field(PREC,"4")
}
record(ao,"$(M)_speed_to_b") {
    field(ASG,"MCC_HARPS")
   field(VAL,"$(scan_speed)")
    field(PREC,"4")
}
record(ao,"$(M)_acq_time") {
    field(ASG,"MCC_HARPS")
   field(VAL,"$(acq_time)")
    field(PREC,"4")
}
record(bo,"$(M)_scan_flag")
{
        field(ASG,"MCC_HARPS")
	field(ZNAM,"none since reboot")
}
