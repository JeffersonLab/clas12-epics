record(calc,"$(P):cTrg:livetime") {
    field(INPA,"$(P):cTrg CPP")
    field(INPB,"$(P):cTrgG CPP")
    field(CALC,"A>0 ? 100*B/A : 0")
    field(EGU,"%")
    field(PREC,1)
}
record(calc,"$(P):cTdc:livetime") {
    field(INPA,"$(P):cTdc CPP")
    field(INPB,"$(P):cTdcG CPP")
    field(CALC,"A>0 ? 100*B/A : 0")
    field(EGU,"%")
    field(PREC,1)
}

