#
# hallb vacuum database, used to make copies of accelerator
# values so that we can place alarms on the vacuum values
#

grecord(ai, "hallb_VMP2C24SPD")
{
	field(INP,"VMP2C24SPD")
	field(HIHI,"102.0")
	field(HIGH,"101.0")
	field(LOW,"90.0")
	field(LOLO,"50.0")
	field(HHSV,"MAJOR")
	field(HSV,"MINOR")
	field(LSV,"MINOR")
	field(LLSV,"MAJOR")
	field(SCAN,"10 second")
	field(PREC,"3")
	field(FLNK,"hallb_VCG2H01A")
  info(autosaveFields_Pass0,"HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

