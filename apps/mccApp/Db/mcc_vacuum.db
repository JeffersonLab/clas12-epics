#
# hallb vacuum database, used to make copies of accelerator
# values so that we can place alarms on the vacuum values
#

grecord(ai, "hallb_VCG2H00ATr")
{
  field(SCAN,"10 second")
	field(INP,"VCG2H00ATr.VAL")
	field(HIHI,"9.0")
	field(HIGH,"0.75")
	field(LOW,"0.05")
	field(LOLO,"0.01")
	field(HHSV,"MAJOR")
	field(HSV,"NO_ALARM")
	field(LSV,"NO_ALARM")
	field(LLSV,"NO_ALARM")
	field(PREC,"3")
	field(FLNK,"hallb_VCG2C24ATr")
  info(autosaveFields_pass0,"HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}
