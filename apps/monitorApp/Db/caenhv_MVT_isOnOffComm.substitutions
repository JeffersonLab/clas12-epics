file "db/caenhv_MVT_isOnOffComm-1type.db" {
pattern { DET MID   TYPE }
        { BMT SEC1_ DRIFT }
        { BMT SEC2_ DRIFT }
        { BMT SEC3_ DRIFT }
        { BMT SEC1_ STRIP }
        { BMT SEC2_ STRIP }
        { BMT SEC3_ STRIP }
        { FMT ""    DRIFT }
        { FMT IN_   STRIP }
        { FMT OUT_  STRIP }
}

file "db/caenhv_BMT_isOnOffComm-1sec.db" {
pattern { SEC }
        { 1 }
        { 2 }
        { 3 }
        { 4 }
        { 5 }
        { 6 }
}
