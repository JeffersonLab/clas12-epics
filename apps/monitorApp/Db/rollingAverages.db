
record(ai,"$(P)$(R=):2h") {
    field(DESC,"Two-Hour Average")
    field(HHSV,"MAJOR")
    field(HSV,"MINOR")
    field(LSV,"MINOR")
    field(LLSV,"MAJOR")
    info(autosaveFields_pass0,"VAL HIHI HIGH LOW LOLO HHSV HSV LSV LLSV")
}
