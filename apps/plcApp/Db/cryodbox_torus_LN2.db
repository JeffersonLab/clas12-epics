record(ai, "$(P)$(R)TC8559M") {
    field(DESC, "Heater Male End")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) TC8559M")
    field(EGU,  "K")
    field(PREC, "2")
}

record(ai, "$(P)$(R)TC8559F") {
    field(DESC, "Heater Female End")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) TC8559F")
    field(EGU,  "K")
    field(PREC, "2")
}
