#
# datatype_ccm_cernox.db - (PLC CCM Record)
#
# Macros:
#   P     - record prefix
#   R     - record prefix
#   PLCID - PLC ID set in startup
#   CCM   - PLC tag (CCM_[A-F])
#
# Author: Wesley Moore (wmoore@jlab.org)
# Date:   Mar 2016
#

record(ai, "$(P)$(R)$(CCM):Cernox_1") {
    field(DTYP, "EtherIP")
    field(INP,  "@$(PLCID) $(CCM).Cernox[1]")
    field(SCAN, "1 second")
    field(EGU,  "K")
    field(PREC, "2")
}

