#
# datatype_ccm_cernox.db - (PLC CCM Record)
#
# Macros:
#   P     - record prefix
#   R     - record prefix
#   PLCID - PLC ID set in startup
#   CCM   - CCM letter only ([A-F])
#
# Author: Wesley Moore (wmoore@jlab.org)
# Date:   Mar 2016
#

record(ai, "$(P)$(R)CCM_$(CCM):TR817$(CCM)1") {
    field(DTYP, "EtherIP")
    field(INP,  "@$(PLCID) CCM_$(CCM).Cernox[1]")
    field(SCAN, "1 second")
    field(EGU,  "K")
    field(PREC, "2")
}

