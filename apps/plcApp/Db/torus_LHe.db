record(ai, "$(P)$(R)PT8111") {
    field(DESC, "LHe Supply Press.")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) PT8111")
    field(EGU,  "psia")
    field(PREC, "2")
}

