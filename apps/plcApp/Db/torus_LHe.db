#
# torus_LHe.db - Torus LHe Cryogenics
#
# Macros:
#   P     - record prefix
#   R     - record prefix
#   PLCID - PLC ID set in startup
#
# Author: Wesley Moore (wmoore@jlab.org)
# Date:   Mar 2016
#

record(ai, "$(P)$(R)TR817U1HB1") {
    field(DESC, "US Hex Beam Cernox")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) HB_US_S1.Temp[1]")
    field(EGU,  "K")
    field(PREC, "2")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

