#
# torus_generic_ai.db - ai Record for Torus PLCs
#
# Macros:
#   P     - record prefix
#   R     - record prefix
#   PLCID - PLC ID set in startup
#   EGU   - (optional)
#   PREC  - (optional, default 0)
#   DESC  - (optional)
#
# Author: Wesley Moore (wmoore@jlab.org)
# Date:   Mar 2016
#

record(ai, "$(P)$(R)$(TAG)") {
    field(DESC, "$(DESC="")")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) $(TAG)")
    field(EGU,  "$(EGU="")")
    field(PREC, "$(PREC=0)")
    info(autosaveFields_pass0, "HIGH HIHI LOW LOLO HSV HHSV LSV LLSV")
}

