record(ai, "$(P)$(R)CG8103") {
    field(DESC, "Upper Torus CC")
    field(DTYP, "EtherIP")
    field(SCAN, "1 second")
    field(INP,  "@$(PLCID) CG8103")
    field(EGU,  "torr")
    field(PREC, "2")
    field(HIGH, "")
    field(HIHI, "")
    field(HSV,  "MINOR")
