#
# torus_wf_generic.db
#
# Generic waveform for Torus FastDAQ testing. 
#
# Macros:
#   P     - record prefix
#   R     - record prefix
#   NELM  - (optional)
#   PREC  - (optional)
#
# Author: Wesley Moore (wmoore@jlab.org)
# Date:   Feb 2017
#

record(waveform, "$(P)$(R)") {
    field(FTVL, "DOUBLE")	
    field(NELM, "$(NELM=2000)")
    field(PREC, "$(PREC=0)")
}
