record(bo,"raster_reset")
{
	field(DESC,"binary output record")
	field(SCAN,"Passive")
	field(PINI,"NO")
	field(VAL,"1")
	field(PHAS,"0")
	field(EVNT,"0")
	field(DTYP,"XYCOM-240M")
	field(SDIS,"0.000000000000000e+00")
	field(DISS, "NO_ALARM")
	field(PRIO, "LOW")
	field(FLNK, "0.000000000000000e+00")
	field(OMSL, "supervisory")
	field(DOL, "0.000000000000000e+00")
	field(OUT, "#C0 S7 @5")   # pin 42 on JK2 connector
	field(HIGH,"0.1")
	field(ZNAM,"OK")
	field(ONAM,"Reset")
	field(ZSV,"NO_ALARM")
	field(OSV,"NO_ALARM")
	field(COSV,"NO_ALARM")
	field(SIOL, "0.000000000000000e+00")
	field(SIML, "0.000000000000000e+00")
	field(SIMS, "NO_ALARM")
	field(IVOA, "Continue normally")
#	field(IVOV, "0.000000000000000e+00")
}
record(bi,"raster_status_ttl")
{
	field(DESC,"binary input record")
	field(SCAN,"1 second")
	field(PINI,"NO")
	field(PHAS,"0")
	field(EVNT,"0")
	field(DTYP,"XYCOM-240M")
	field(DISV,"1")
	field(SDIS,"0.000000000000000e+00")
	field(DISS, "NO_ALARM")
	field(PRIO, "LOW")
	field(FLNK, "0.000000000000000e+00")
	field(INP, "#C0 S2 @7")   # pin 33 (or 32???) on JK1 connector
	field(ZNAM,"Stopped")
	field(ONAM,"Running")
	field(ZSV,"NO_ALARM")
	field(OSV,"NO_ALARM")
	field(COSV,"NO_ALARM")
	field(SIOL, "0.000000000000000e+00")
	field(SIML, "0.000000000000000e+00")
	field(SIMS, "NO_ALARM")

}
