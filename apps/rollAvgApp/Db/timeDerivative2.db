record(scalcout,"$(P):units") {
    field(PINI,"1")
    field(INAA,"$(INP).EGU")
    field(CALC,"AA+'/s'")
    field(OUT,"$(P).EGU")
}

record(compress,"$(P):fifo") {
    field(SCAN,"$(NSEC) second")
    field(INP,"$(INP)")
    field(NSAM,"$(N)")
    field(ALG,"Circular Buffer")
}

record(acalcout,"$(P)") {
    field(NELM,"$(N)")
    field(INAA,"$(P):fifo CPP")
    field(CALC,"AVG(DERIV(AA))/$(NSEC)")
}

