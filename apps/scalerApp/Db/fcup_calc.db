#
# fcup_calc.db - was in scaler.db
#
# Faraday Cup calculation
#
# Macros:
#   SCLR - scaler channel to use (counts/sec)
#

# INPA - scaler counts/sec
record(calc,"$(P)$(R)") {
	field(DESC, "FCUP Calcuation")
    field(CALC, "(a-200)/905.937")
	field(INPA, "$(SCLR)")
	field(PREC, "3")
	field(SCAN, ".5 second")
}

