#
# fcup_calc.db - was in scaler.db
#
# Faraday Cup calculation
#
# Macros:
#   SCLR - scaler channel to use
#

# INPA - scaler count
# INPB - offset
# INPC - slope
# INPD - scaler time
record(calc,"HB:fcup") {
	field(DESC, "FCUP Calcuation")
    field(CALC, "(a/b-c)/max(d,1.0)")
	field(INPA, "$(SCLR).$(CH)")
    field(INPB, "$(SCLR).T")
    field(C, "200")
	field(D, "905.937")
	field(PREC, "3")
	field(SCAN, ".5 second")
}

