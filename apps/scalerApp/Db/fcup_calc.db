#
# fcup_calc.db - was in scaler.db
#
# Faraday Cup calculation
#
# Macros:
#   SCLR - scaler channel to use
#

# INPA - scaler count
# INPB - offset
# INPC - slope
# INPD - scaler time
record(calc,"HB:fcup") {
	field(DESC, "FCUP Calcuation")
    field(CALC, "(a/d-b)/MAX(c,1.0)")
	field(INPA, "$(SCLR) NPP NMS")
	field(INPB, "200")
	field(INPC, "905.937")
	field(INPD, "$(SCLR).T NPP NMS")
	field(PREC, "3")
	field(SCAN, ".5 second")
}

