	field(INPD,"scaler.S4 NPP NMS")
	field(INPE,"scaler.S5 NPP NMS")

	field(OUTG,"scalerS2a.VAL PP MS")
	field(OUTH,"scalerS3a.VAL PP MS")
	field(OUTI,"scalerS4a.VAL PP MS")
	field(OUTJ,"scalerS5a.VAL PP MS")


	field(PREC,"3")
}

