record(calc,"scaler_calc1b") {
    field(INPA,"scaler_calc1 CPP")
    field(INPB,"beam_stop_at_beam CP")
    field(INPC,"beam_stop_atten_rge")
    field(CALC,"ABS(B)>0.1 ? A*C : A")
    field(PREC,"3")
}

record(sel,"beam_stop_atten_rge") {
    field(SCAN,"2 second")
