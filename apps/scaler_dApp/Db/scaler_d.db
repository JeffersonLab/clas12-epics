grecord(scaler,"scaler_d")
{
#	field(SCAN,"I/O Intr")
#	field(SCAN,"Passive")
	field(DTYP,"Joerger VSC8/16")
	field(FLNK,"scaler_d_calc1.PROC PP MS")
	field(FREQ,"10000000")
	field(OUT,"#C1 S0 @")
	field(TP,"2")
	field(PREC,"3")
	field(G1,"1") 		# The gate 1 is a preset gate
	field(NM1,"Clock") 	# Channel 1 : clock
}

