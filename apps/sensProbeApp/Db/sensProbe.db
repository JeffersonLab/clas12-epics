record(ai, "$(P)$(R)Temp") {
    field(DTYP, "Snmp")
    field(INP,  "@$(HOST) public SPAGENT-MIB::sensorProbeTempDegree.1 Float: 100")
    field(PREC, "4")
    field(EGU,  "F")
    field(SCAN, "10 second")
}

record(ai, "$(P)$(R)Humidity") {
    field(DTYP, "Snmp")
    field(INP,  "@$(HOST) public SPAGENT-MIB::sensorProbeHumidityPercent.1 Float: 100")
    field(PREC, "4")
    field(EGU,  "%")
    field(SCAN, "10 second")
}

