#
# sensorProbe_humidity.db
# AKCP sensorProbe - Humidity Sensor
#

record(ai, "$(P)$(R)$(C)_Humid") {
    field(DTYP, "Snmp")
    field(INP,  "@$(HOST) public %(MIB)HumidityPercent.$(C) INTEGER: 100")
    field(EGU,  "%")
    field(SCAN, "$(SCAN)")
    info(autosaveFields_pass0,"HIHI HIGH LOW LOLO HHSV HSV LSV LLSV")
}

