record(calc,"B_SVT_TEMP_$(M8):alarm") {
	field(SCAN,"1 second")
	field(CALC,"A>1||B>1")
	field(INPA,"B_SVT_TEMP_$(M8)T_Slot$(S)Alm.SEVR")
	field(INPB,"B_SVT_TEMP_$(M8)B_Slot$(S)Alm.SEVR")
	field(HIHI,"1")
	field(HHSV,"MAJOR")
}
