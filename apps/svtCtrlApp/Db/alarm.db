record(calc,"LV_$(module):alarm") {
	field(SCAN,"1 second")
	field(CALC,"A || B || C || D")
	field(INPA,"$(LVAT):modErr")
	field(INPB,"$(LVDT):modErr")
	field(INPC,"$(LVAB):modErr")
	field(INPD,"$(LVDB):modErr")
	field(HIHI,"0.7")
	field(HHSV,"MAJOR")
}
record(calc,"HV_$(module):alarm") {
	field(SCAN,"1 second")
	field(CALC,"A || B ")
	field(INPA,"$(HVT):modErr")
	field(INPB,"$(HVB):modErr")
	field(HIHI,"0.7")
	field(HHSV,"MAJOR")
}
