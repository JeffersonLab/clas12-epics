  field(INPC,"B_SVT_LV_VD_R$(R)S$(S)T_Slot$(LSL):outswitchR CPP")
  field(INPD,"B_SVT_LV_VD_R$(R)S$(S)B_Slot$(LSL):outswitchR CPP")
  field(CALC,"(A=0 && B=0 && C=0 && D=0) ? 1 : 0")
  field(HHSV,"MAJOR")
