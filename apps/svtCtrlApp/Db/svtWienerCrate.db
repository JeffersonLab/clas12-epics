	field(OUT,"@$(HOST) guru %{W}sysMainSwitch.0 ( 100 i")
    field(ASG,"ALLWRITE")
}
record(mbbiDirect,"$(HOST):systemStatus") {
	field(DTYP,"Soft Channel")
	field(FLNK,"$(HOST):mainOn")
