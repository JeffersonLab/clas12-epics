#
# EPICS records for all 
#

record( ai, "$(PREF)glob:min_rpm_vme" ) {
	 field( DESC, "Min RPM for VME" )
	 field( SCAN, "Passive" )
	 field( EGU , "RPM" )
	 field( VAL , "$(MINVME)" )
	 field( PINI, "YES")
}
record( ai, "$(PREF)glob:max_rpm_vme" ) {
	 field( DESC, "Max RPM for VME" )
	 field( SCAN, "Passive" )
	 field( EGU , "RPM" )
	 field( VAL , "$(MAXVME)" )
	 field( PINI, "YES")
}
