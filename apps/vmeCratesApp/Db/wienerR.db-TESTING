record (longin,"$(HOST):fanAirTemp")
{
	field (DTYP,"Snmp")
	field (INP,"@$(IP) $(COMMUNITY) %(WF)AirTemperature.0 INTEGER: 100")
	field (SCAN,"10 second")
	field (DESC,"Temperature in C of fan tray inlet air")
}
