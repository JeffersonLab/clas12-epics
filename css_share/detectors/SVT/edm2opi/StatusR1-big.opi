  <widget typeId="org.csstudio.opibuilder.widgets.Rectangle" version="1.0">
    <border_alarm_sensitive>false</border_alarm_sensitive>
    <line_width>1</line_width>
    <x>375</x>
    <y>357</y>
    <width>97</width>
    <height>27</height>
    <name>EDM Rectangle</name>
    <line_color>
      <color blue="0" green="0" name="black-14" red="0"/>
    </line_color>
    <transparent>false</transparent>
    <background_color>
      <color blue="184" green="198" name="LINAC title" red="185"/>
    </background_color>
    <visible>true</visible>
    <rules>
      <rule name="lineColorAlarmRule" out_exp="false" prop_id="line_color">
        <exp bool_exp="pvSev0==-1">
          <value>
            <color name="Invalid"/>
          </value>
        </exp>
        <exp bool_exp="pvSev0==1">
          <value>
            <color name="Major"/>
          </value>
        </exp>
        <exp bool_exp="pvSev0==2">
          <value>
            <color name="Minor"/>
          </value>
        </exp>
        <pv trig="true">B_SVT_TEMP_R1S1:alarm</pv>
      </rule>
      <rule name="backColorAlarmRule" out_exp="false" prop_id="background_color">
        <exp bool_exp="pvSev0==-1">
          <value>
            <color name="Invalid"/>
          </value>
        </exp>
        <exp bool_exp="pvSev0==1">
          <value>
            <color name="Major"/>
          </value>
        </exp>
        <exp bool_exp="pvSev0==2">
          <value>
            <color name="Minor"/>
          </value>
        </exp>
        <pv trig="true">B_SVT_TEMP_R1S1:alarm</pv>
      </rule>
    </rules>
    <line_style>0</line_style>
  </widget>
  <widget typeId="org.csstudio.opibuilder.widgets.Label" version="1.0">
    <border_alarm_sensitive>false</border_alarm_sensitive>
    <x>391</x>
    <y>364</y>
    <width>65</width>
    <height>16</height>
    <font>
      <fontdata fontName="helvetica" height="14" pixels="true" style="0"/>
